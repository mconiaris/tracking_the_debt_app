<h1>This is fiscal_years home.html</h1>
<div id="graphdiv"></div>
<script type="text/javascript">
  g = new Dygraph(

    // containing div
    document.getElementById("graphdiv"),

    // CSV or path to a CSV file.
    <%= current_dollars %>,
              {
                labels: [ "Fiscal Year", "Receipts", "Outlays" ],
      drawPoints: true,
      xlabel:"Fiscal Year",
      ylabel:"Percentage Surplus or Deficit",
      title: "Summary of Receipts, Outlays, and Surpluses or Deficits\nIn Current Dollars: 1940-2019"
    }
              });
</script>

