<article>
  <p id="notice"><%= notice %></p>

  <h2>
    <strong>Fiscal year:</strong>
    <%= @fiscal_year.fiscal_year %>
  </h2>

  <p>
    <strong>Receipts current dollars:</strong>
    <%= @fiscal_year.receipts_current_dollars %>
  </p>

  <p>
    <strong>Outlays current dollars:</strong>
    <%= @fiscal_year.outlays_current_dollars %>
  </p>

  <p>
    <strong>Surplus deficit current dollars:</strong>
    <%= @fiscal_year.surplus_deficit_current_dollars %>
  </p>

  <p>
    <strong>Receipts constant:</strong>
    <%= @fiscal_year.receipts_constant %>
  </p>

  <p>
    <strong>Outlays constant:</strong>
    <%= @fiscal_year.outlays_constant %>
  </p>

  <p>
    <strong>Surplus deficit constant:</strong>
    <%= @fiscal_year.surplus_deficit_constant %>
  </p>

  <p>
    <strong>GDP receipts:</strong>
    <%= @fiscal_year.gdp_receipts %>
  </p>

  <p>
    <strong>GDP outlays:</strong>
    <%= @fiscal_year.gdp_outlays %>
  </p>

  <p>
    <strong>GDP surplus deficit:</strong>
    <%= @fiscal_year.gdp_surplus_deficit %>
  </p>

  <% if session[:access_token] %>
    <%= link_to 'Edit', edit_fiscal_year_path(@fiscal_year) %> |
  <% end %>
  <%= link_to 'Back', fiscal_years_path %>
</article>
